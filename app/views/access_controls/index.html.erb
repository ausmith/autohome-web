<h1>Listing users_access_controls</h1>

<table>
  <tr>
    <th>Access control type</th>
    <th>Value</th>
    <th>Description</th>
    <th>Enabled</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @access_controls.each do |access_control| %>
  <tr>
    <td><%= access_control.access_control_type_id %></td>
    <td><%= access_control.value %></td>
    <td><%= access_control.description %></td>
    <td><%= access_control.enabled %></td>
    <td><%= link_to 'Show', user_access_control_path(access_control.user_id, access_control) %></td>
    <td><%= link_to 'Edit', edit_user_access_control_path(access_control.user_id, access_control) %></td>
    <td><%= link_to 'Destroy', user_access_control_path(access_control.user_id, access_control), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Access control', new_user_access_control_path(@user) %>
